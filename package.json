    "tsup": "^6.6.3",
    "turndown": "^7.1.2",
    "typescript": "^4.4.2",
    "unplugin-auto-import": "^0.4.5",
    "vite": "^3.2.4",
    "vite-plugin-svgr": "^2.4.0",
    "web-ext": "^6.3.0",
    "webext-bridge": "^4.1.1",
    "webextension-polyfill": "^0.8.0"
  }
}
